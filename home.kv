#:kivy 2.1.0
#:include menu.kv

<HomeScreen@MDScreen>:
    name: 'HomeScreen'
    BoxLayout:
        orientation: "vertical"
        Toolbar:
            title: "Home"
        MDCard:
            size_hint: 0.4, None
            pos_hint: {'center_x':0.5}
            MDLabel:
                id: time_label
                font_size: 100
                halign: 'center'
                font_name: 'fonts/digital-7.ttf'
        SpDial:
            data: root.data
        Widget:
            size_hint_y: 0.5